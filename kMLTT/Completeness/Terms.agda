{-# OPTIONS --without-K --safe #-}

module kMLTT.Completeness.Terms where

-- v-≈        : ∀ {x} →
--              ⊨ Γ →
--              x ∶ T ∈! Γ →
--              -----------------
--              Γ ⊨ v x ≈ v x ∶ T
-- []-cong    : Δ ⊨ t ≈ t′ ∶ T →
--              Γ ⊨s σ ≈ σ′ ∶ Δ →
--              ---------------------------------
--              Γ ⊨ t [ σ ] ≈ t′ [ σ′ ] ∶ T [ σ ]
-- [I]        : Γ ⊨ t ∶ T →
--              --------------------
--              Γ ⊨ t [ I ] ≈ t ∶ T
-- [p]        : ∀ {x} →
--              Δ ⊨s σ ∶ S ∺ Γ →
--              x ∶ T ∈! Γ →
--              ---------------------------------------------
--              Δ ⊨ v x [ p σ ] ≈ v (suc x) [ σ ] ∶ T [ p σ ]
-- [∘]        : Γ ⊨s τ ∶ Γ′ →
--              Γ′ ⊨s σ ∶ Γ″ →
--              Γ″ ⊨ t ∶ T →
--              ---------------------------------------------
--              Γ ⊨ t [ σ ∘ τ ] ≈ t [ σ ] [ τ ] ∶ T [ σ ∘ τ ]
-- [,]-v-ze   : ∀ {i} →
--              Γ ⊨s σ ∶ Δ →
--              Δ ⊨ S ∶ Se i →
--              Γ ⊨ s ∶ S [ σ ] →
--              --------------------------------
--              Γ ⊨ v 0 [ σ , s ] ≈ s ∶ S [ σ ]
-- [,]-v-su   : ∀ {i x} →
--              Γ ⊨s σ ∶ Δ →
--              Δ ⊨ S ∶ Se i →
--              Γ ⊨ s ∶ S [ σ ] →
--              x ∶ T ∈! Δ →
--              ---------------------------------------------
--              Γ ⊨ v (suc x) [ σ , s ] ≈ v x [ σ ] ∶ T [ σ ]
-- ≈-conv     : ∀ {i} →
--              Γ ⊨ s ≈ t ∶ S →
--              Γ ⊨ S ≈ T ∶ Se i →
--              --------------------
--              Γ ⊨ s ≈ t ∶ T
-- ≈-sym      : Γ ⊨ t ≈ t′ ∶ T →
--              ----------------
--              Γ ⊨ t′ ≈ t ∶ T
-- ≈-trans    : Γ ⊨ t ≈ t′ ∶ T →
--              Γ ⊨ t′ ≈ t″ ∶ T →
--              ------ ------------
--              Γ ⊨ t ≈ t″ ∶ T
